---
import Layout from '@layouts/ExamplesLayout.astro';
import { BlockPreview } from '@components/BlockPreview';
import { storePreview } from '@components/examples/ecommerce/HomePage';
import { productPreview } from '@components/examples/ecommerce/ProductPage';

const examples = [storePreview, productPreview];
---

<Layout title="Ecommerce Examples" description="Ecommerce block examples including built with Tailus UI React" image="/ogs/ecommerce-og.png">
    {examples.map(example => <BlockPreview client:visible {...example} />)}
</Layout>
