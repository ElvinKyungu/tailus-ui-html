---
import Layout from '@layouts/ExamplesLayout.astro';
import { BlockPreview } from '@components/BlockPreview';
import { login1Preview } from '@components/examples/forms/Login1';
import { hero2Preview } from '@components/examples/marketing/Hero2';
import { storePreview } from '@components/examples/ecommerce/HomePage';
import { cardPreview } from '@components/examples/layouts';
const examples = [cardPreview, hero2Preview, storePreview, login1Preview];
---

<Layout title="UI Examples" description="Application Layouts, Login pages, Sign Up pages and Marketing blocks examples built with Tailus UI React" breadcrumb={false}>
    {examples.map(example => <BlockPreview client:visible {...example} asCategoryPreview />)}
</Layout>
