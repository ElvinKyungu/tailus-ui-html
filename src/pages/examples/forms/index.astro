---
import Layout from '@layouts/ExamplesLayout.astro';
import { BlockPreview } from '@components/BlockPreview';
import { login1Preview } from '@components/examples/forms/Login1';
import { login2Preview } from '@components/examples/forms/Login2';
import { login4Preview } from '@components/examples/forms/Login4';
import { login3Preview } from '@components/examples/forms/Login3';
import { register1Preview } from '@components/examples/forms/Register1';
import { register2Preview } from '@components/examples/forms/Register2';
import { register3Preview } from '@components/examples/forms/Register3';
import { register4Preview } from '@components/examples/forms/Register4';

const examples = [login1Preview, login2Preview, login3Preview, login4Preview, register1Preview, register2Preview, register3Preview, register4Preview];
---

<Layout title="Forms Examples" description="Login and Sign Up page examples built with Tailus UI React" image="/ogs/forms-og.png">
    {examples.map(example => <BlockPreview client:visible {...example} />)}
</Layout>
